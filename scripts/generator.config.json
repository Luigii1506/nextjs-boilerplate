  "validation": {
    "moduleNamePattern": "^[a-z][a-z0-9-]*$",
    "maxModuleNameLength": 50,
    "maxDisplayNameLength": 100,
    "maxDescriptionLength": 500,
    "maxPrismaFields": 20
  },
  "features": {
    "supportBothModuleTypes": true,
    "autoIntegrateNavigation": true,
    "autoUpdateFeatureFlags": true,
    "generatePrismaSchema": true,
    "generateUIComponents": true,
    "generateServerActions": true,
    "generateValidationSchemas": true,
    "generateBarrelExports": true,
    "generateDocumentation": true,
    "formatGeneratedCode": true
  }
}
