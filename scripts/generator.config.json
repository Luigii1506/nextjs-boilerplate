{
  "generator": {
    "version": "2.0.0",
    "name": "Enterprise Module Generator",
    "description": "Generador automatizado de m√≥dulos Enterprise siguiendo ENTERPRISE_PATTERNS.md"
  },
  "defaults": {
    "moduleType": "feature",
    "category": "feature",
    "requiredRole": "user",
    "order": 50,
    "icon": "Settings",
    "enablePrisma": true,
    "enableUI": true,
    "enableTests": true
  },
  "paths": {
    "coreModules": "src/features",
    "featureModules": "src/modules",
    "navigation": "src/core/navigation/constants.ts",
    "featureFlags": "src/core/config/feature-flags.ts",
    "prismaSchema": "src/core/database/prisma/schema.prisma"
  },
  "patterns": {
    "enforceEnterprisePatterns": true,
    "requireDocumentation": true,
    "autoFormatCode": true,
    "validateTypeScript": true,
    "generateTests": false
  },
  "ui": {
    "availableIcons": [
      "Home",
      "Users",
      "Upload",
      "Sliders",
      "Database",
      "Shield",
      "Bell",
